extname=istore

declare -A istore=(
    [name]=IStore
    [keysize]=4
    [valsize]=4
    [key_sql_type]=int4
    [key_in_f]=int4in
    [key_out_f]=int4out
    [key_cmp_f]=btint4cmp)

declare -A bigistore=(
    [name]=BigIStore
    [keysize]=4
    [valsize]=8
    [key_sql_type]=int4
    [key_in_f]=int4in
    [key_out_f]=int4out
    [key_cmp_f]=btint4cmp)

declare -a types=( istore bigistore )
